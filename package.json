{
  "name": "changbo-web",
  "version": "1.0.0",
  "private": true,
  "description": "畅博管理系统",
  "author": "zhihao",
  "scripts": {
    "clean": "rm -rf node_modules .umi src/.umi",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider UMI_ENV=dev BUILD_ENV=dev umi dev",
    "build:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider BUILD_ENV=prod umi build && yarn generate:version",
    "build:test": "cross-env NODE_OPTIONS=--openssl-legacy-provider BUILD_ENV=dev umi build && yarn generate:version",
    "build:staging": "cross-env NODE_OPTIONS=--openssl-legacy-provider BUILD_ENV=staging umi build && yarn generate:version",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "stylelint": "stylelint src/**/* --fix --color --custom-syntax postcss-less",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write 'src/**/*' --end-of-line auto",
    "lint:style": "stylelint --fix 'src/**/*.less' --fix --color --custom-syntax postcss-less",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "tsc": "tsc --noEmit",
    "prepare": "husky install",
    "generate:version": "EXTERNAL_PATH=./src/version.json generate-version-file"
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix --color --custom-syntax postcss-less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": "prettier --write"
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.5.0",
    "@ant-design/charts": "^1.4.3",
    "@ant-design/icons": "^4.8.3",
    "@ant-design/pro-components": "^2.6.11",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@types/lodash": "^4.14",
    "@wangeditor/editor": "^5.1.14",
    "@wangeditor/editor-for-react": "^1.0.4",
    "ahooks": "^3.7.0",
    "antd": "^4.24.13",
    "axios": "^1.12.0",
    "bignumber.js": "^9.1.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pino": "^9.12.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^17.0.2",
    "umi": "^3.5.32",
    "version-rocket": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@umijs/fabric": "^2.8.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.32",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^10.0.7",
    "postcss": "^8.4.16",
    "postcss-less": "^6.0.0",
    "prettier": "^2.2.0",
    "react-dev-inspector": "^1.8.1",
    "stylelint": "^14.5.0",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "4.9.5",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {},
  "resolutions": {
    "minimatch": "9.0.5"
  }
}
